{
	"name": "sveltekit-fire",
	"version": "0.0.16",
	"scripts": {
		"dev": "vite dev",
		"dev:em": "cross-env VITE_USE_FIREBASE_EMULATOR=true vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package",
		"publish": "cd package && npm publish",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"deploy": "npm run build && firebase deploy"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.44",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.7",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"autoprefixer": "^10.3.7",
		"concurrently": "^6.4.0",
		"cross-env": "^7.0.3",
		"cssnano": "^5.0.11",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"firebase": "^9.6.0",
		"npm": "^8.1.4",
		"postcss": "^8.3.9",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"prism-svelte": "^0.4.7",
		"prismjs": ">=1.27.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0",
		"tailwindcss": "^3.0.23"
	},
	"type": "module",
	"description": "TODO",
	"main": "index.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/Leonavas/sveltekit-fire"
	},
	"keywords": [
		"sveltekit",
		"firebase",
		"firestore"
	],
	"homepage": "https://sveltekitfire.web.app",
	"author": "Rodrigo Leonavas",
	"license": "ISC"
}
